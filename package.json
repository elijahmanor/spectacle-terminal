{
  "name": "spectacle-terminal",
  "version": "0.5.0",
  "description": "Terminal component that can be used in a spectacle slide deck",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "rm -rf lib/",
    "build": "babel src -d lib",
    "prepublish": "npm run build && rm -rf src",
    "postpublish": "git checkout -- .",
    "postinstall": "node -e \"require('fs').stat('lib', function (e, s) { process.exit(e || !s.isDirectory() ? 1 : 0) })\" || npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elijahmanor/spectacle-terminal.git"
  },
  "keywords": [
    "spectacle",
    "terminal"
  ],
  "author": "Elijah Manor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elijahmanor/spectacle-terminal/issues"
  },
  "homepage": "https://github.com/elijahmanor/spectacle-terminal#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "eslint": "^3.12.1",
    "eslint-config-leankit": "^3.0.0",
    "spectacle": "^1.2.5"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
